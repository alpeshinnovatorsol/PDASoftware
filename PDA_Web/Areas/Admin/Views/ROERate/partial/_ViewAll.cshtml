@model IEnumerable<PDAEstimator_Domain.Entities.ROERates>
<style>
    table td {
        width: 110px;
        word-wrap: break-word;
    }
</style>
<table class="table table-bordered table-striped" id="ROERateTable">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Roename)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CurrencyName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ROERate)
            </th>
            @*<th>
                    @Html.DisplayNameFor(model => model.UserId)
            </th>
            <th>
                    @Html.DisplayNameFor(model => model.CreationDate)
            </th>*@
            
            
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <th>
                    @Html.DisplayFor(modelitem => item.Roename)
                </th>
                <th>
                    @Html.DisplayFor(modelitem => item.CurrencyName)
                </th>
                <th>
                    @Html.DisplayFor(modelitem => item.ROERate)
                </th>
              
            </tr>
        }
    </tbody>
</table>
